<h1>List of Referal Agent Signatory</h1>

 <table class=" myTable table table-hover">
  <thead>
    <tr style="">
      <th>Name</th>
      <th>email</th>
      <th>Contact</th>
      <th>City</th>
      <th>Gender</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ras.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.phone %></td>
        <td><%= user.city %></td>
        <td><%= user.gender %></td>
        <td><%= user.address %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>